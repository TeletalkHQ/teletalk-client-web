{
  "name": "teletalk-client",
  "author": "S_STALWART_S",
  "description": "teletalk messenger app (client)",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/S-STALWART-S/teletalk-client"
  },
  "scripts": {
    "build": "npm run production:build",
    "check:format": "prettier --write ./",
    "check:lint": "eslint ./",
    "check:lint:default": "eslint 'src/**/*.js' 'src/**/*.jsx'",
    "check:security": "snyk test & snyk code test",
    "check:style": "npm run check:format && npm run check:lint",
    "dev": "cross-env npm run start:dev",
    "eject": "react-scripts eject",
    "git:prepare": "husky install",
    "production:build": "cross-env REACT_APP_RUNTIME_MODE=production react-scripts build",
    "production:deploy": "gh-pages -d build",
    "start": "cross-env npm run start:production",
    "start:dev": "cross-env REACT_APP_RUNTIME_MODE=development PORT=${PORT:-4090} react-scripts start",
    "start:dev:1": "cross-env PORT=4091 REACT_APP_DEVELOPMENT_SERVER_BASE_URL_INDEX=1 npm run start:dev",
    "start:dev:2": "cross-env PORT=4092 REACT_APP_DEVELOPMENT_SERVER_BASE_URL_INDEX=2 npm run start:dev",
    "start:production": "cross-env REACT_APP_RUNTIME_MODE=production PORT=${PORT:-4080} react-scripts start",
    "start:production1": "cross-env PORT=4081 REACT_APP_PRODUCTION_SERVER_BASE_URL_INDEX=1 npm run start:production",
    "start:production2": "cross-env PORT=4082 REACT_APP_PRODUCTION_SERVER_BASE_URL_INDEX=2 npm run start:production",
    "test": "react-scripts test"
  },
  "keywords": [
    "teletalk",
    "messenger"
  ],
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "5.0.0-alpha.112",
    "@mui/material": "^5.11.0",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.2.1",
    "cross-env": "^7.0.3",
    "custom-typeof": "^1.2.0",
    "fastest-validator": "^1.16.0",
    "immer": "^9.0.16",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "logger-browser": "^1.1.0",
    "nanoid": "^4.0.0",
    "notistack": "^3.0.0-alpha.11",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-scripts": "^5.0.1",
    "react-scroll-up": "^1.4.0",
    "react-spinners": "^0.13.7",
    "socket.io-client": "^4.5.4",
    "typescript": "^4.9.4",
    "utility-store": "^8.1.0",
    "web-vitals": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@cspell/eslint-plugin": "^6.17.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-sonarjs": "^0.17.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "./src/*": [
      "eslint",
      "prettier --write"
    ]
  }
}
