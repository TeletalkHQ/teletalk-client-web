{
  "name": "teletalk-client-web",
  "author": "S_STALWART_S",
  "description": "teletalk messenger app (client)",
  "license": "GPL-3",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/S-STALWART-S/teletalk-client"
  },
  "scripts": {
    "build": "next build",
    "check:format": "prettier --write --list-different ./",
    "check:lint": "eslint ./ --fix",
    "check:security": "snyk test & snyk code test",
    "check:style": "npm run check:format && npm run check:lint",
    "check:types": "tsc --noEmit",
    "dev": "npm run start:dev",
    "git:prepare": "husky install",
    "lint": "next lint",
    "production:build": "cross-env NEXT_PUBLIC_RUNTIME_MODE=production react-scripts build",
    "production:deploy": "gh-pages -d build",
    "production:prebuild": "node --loader ./loader.js ./scripts/prebuild/index.ts",
    "setup:alias:load": "./scripts/alias/loader.sh",
    "setup:alias:unload": "./scripts/alias/unloader.sh",
    "start": "start:production",
    "start:dev": "cross-env NEXT_PUBLIC_RUNTIME_MODE=development PORT=${PORT:-4090} next dev",
    "start:dev:1": "cross-env PORT=4091 NEXT_PUBLIC_DEVELOPMENT_SERVER_BASE_URL_INDEX=1 npm run start:dev",
    "start:dev:2": "cross-env PORT=4092 NEXT_PUBLIC_DEVELOPMENT_SERVER_BASE_URL_INDEX=2 npm run start:dev",
    "start:production": "cross-env NEXT_PUBLIC_RUNTIME_MODE=production PORT=${PORT:-4080} next start",
    "start:production1": "cross-env PORT=4081 NEXT_PUBLIC_PRODUCTION_SERVER_BASE_URL_INDEX=1 npm run start:production",
    "start:production2": "cross-env PORT=4082 NEXT_PUBLIC_PRODUCTION_SERVER_BASE_URL_INDEX=2 npm run start:production",
    "test": "react-scripts test"
  },
  "keywords": [
    "teletalk",
    "messenger"
  ],
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "5.0.0-alpha.132",
    "@mui/material": "^5.13.4",
    "@mui/styles": "^5.13.2",
    "@tanstack/react-query": "^4.29.5",
    "@types/jest": "^29.5.2",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.4.0",
    "check-fields": "^3.0.0",
    "cross-env": "^7.0.3",
    "custom-typeof": "^3.0.4",
    "fastest-validator": "^1.17.0",
    "immer": "^10.0.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "logger-browser": "^2.0.2",
    "next": "13.4.4",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-scripts": "^5.0.1",
    "react-scroll-up": "^1.4.0",
    "react-spinners": "^0.13.8",
    "simple-trier": "^4.0.3",
    "socket.io-client": "^4.6.2",
    "typescript": "5.1.3",
    "utility-store": "^12.2.3",
    "web-vitals": "^3.3.2",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@cspell/eslint-plugin": "^6.31.1",
    "@rushstack/eslint-patch": "^1.3.1",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@tanstack/react-query-devtools": "^4.29.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/autosuggest-highlight": "^3.2.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/resolve": "^1.20.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "circular-dependency-plugin": "^5.2.2",
    "dotenv": "^16.1.4",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.21.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "resolve": "^1.22.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "./src/*": [
      "eslint",
      "prettier --write"
    ]
  }
}
