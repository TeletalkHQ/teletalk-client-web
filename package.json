    "@tanstack/react-query": "^5.12.2",
    "@types/jest": "^29.5.11",
    "autosuggest-highlight": "^3.3.4",
    "await-timeout": "^1.1.1",
    "axios": "^1.6.2",
    "check-fields": "^5.0.1",
    "compressorjs": "^1.2.1",
    "cross-env": "^7.0.3",
    "custom-typeof": "^3.0.4",
    "fastest-validator": "^1.17.0",
    "howler": "^2.2.4",
    "immer": "^10.0.3",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "logger-browser": "^2.0.4",
    "next": "14.0.4",
    "notistack": "^3.0.1",
    "p-timeout": "^6.1.2",
