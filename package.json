{
  "name": "teletalk-client-web",
  "author": "S_STALWART_S",
  "description": "teletalk messenger app (client)",
  "license": "GPL-3",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/S-STALWART-S/teletalk-client"
  },
  "scripts": {
    "build": "next build",
    "check:format": "prettier --write ./",
    "check:lint": "eslint ./",
    "check:security": "snyk test & snyk code test",
    "check:style": "npm run check:format && npm run check:lint",
    "dev": "npm run start:dev",
    "git:prepare": "husky install",
    "lint": "next lint",
    "production:build": "cross-env REACT_APP_RUNTIME_MODE=production react-scripts build",
    "production:deploy": "gh-pages -d build",
    "start": "start:production",
    "start:dev": "cross-env REACT_APP_RUNTIME_MODE=development PORT=${PORT:-4090} next dev",
    "start:dev:1": "cross-env PORT=4091 REACT_APP_DEVELOPMENT_SERVER_BASE_URL_INDEX=1 npm run start:dev",
    "start:dev:2": "cross-env PORT=4092 REACT_APP_DEVELOPMENT_SERVER_BASE_URL_INDEX=2 npm run start:dev",
    "start:production": "cross-env REACT_APP_RUNTIME_MODE=production PORT=${PORT:-4080} next start",
    "start:production1": "cross-env PORT=4081 REACT_APP_PRODUCTION_SERVER_BASE_URL_INDEX=1 npm run start:production",
    "start:production2": "cross-env PORT=4082 REACT_APP_PRODUCTION_SERVER_BASE_URL_INDEX=2 npm run start:production",
    "test": "react-scripts test"
  },
  "keywords": [
    "teletalk",
    "messenger"
  ],
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "5.0.0-alpha.123",
    "@mui/material": "^5.11.13",
    "@mui/styles": "^5.11.13",
    "@types/jest": "^29.5.1",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^1.3.4",
    "check-fields": "^1.2.4",
    "cross-env": "^7.0.3",
    "custom-typeof": "^3.0.4",
    "fastest-validator": "^1.16.0",
    "immer": "^9.0.19",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "logger-browser": "^1.1.2",
    "next": "13.4.3",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-redux": "^8.0.5",
    "react-scripts": "^5.0.1",
    "react-scroll-up": "^1.4.0",
    "react-spinners": "^0.13.8",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "simple-trier": "^3.1.1",
    "socket.io-client": "^4.6.1",
    "typescript": "^5.0.2",
    "utility-store": "^12.0.0",
    "web-vitals": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@cspell/eslint-plugin": "^6.30.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "20.2.3",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.3",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "typescript": "5.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "./src/*": [
      "eslint",
      "prettier --write"
    ]
  }
}
