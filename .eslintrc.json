{
  "extends": ["react-app", "react-app/jest"],
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "logger": true
  },
  "plugins": ["spellcheck"],
  "rules": {
    "spellcheck/spell-checker": [
      1,
      {
        "comments": true,
        "strings": true,
        "identifiers": true,
        "ignoreRequire": false,
        "templates": true,
        "lang": "en_US",
        "skipWords": [
          "abcdefghijklmnopqrstufwxyz",
          "abcdefghijklmnopqrstufwxyzABCDEFGHIJKLMNOPQRSTUFWXYZ1234567890",
          "Alphadash",
          "Alphanum",
          "Apis",
          "arrayEnum",
          "autofetch",
          "autopep",
          "axios",
          "bcrypt",
          "bcryptjs",
          "bgcolor",
          "camelcase",
          "chai",
          "CLEANME",
          "codestream",
          "cors",
          "dotenv",
          "endregion",
          "enqueue",
          "enum",
          "enumValue",
          "esbenp",
          "favicon",
          "fileupload",
          "Fira",
          "flowtype",
          "func",
          "indenticator",
          "ioredis",
          "ipynb",
          "iran",
          "Jalali",
          "jsonwebtoken",
          "luhn",
          "maxlength",
          "Melipayamak",
          "middlewares",
          "MINLENGTH",
          "monospace",
          "urlencoded",
          "jpg",
          "instanceof",
          "morgan",
          "multer",
          "nanoid",
          "nodemailer",
          "onum",
          "quatro",
          "redis",
          "Replacer",
          "responser",
          "restclient",
          "Restify",
          "sendable",
          "sindresorhus",
          "snackbar",
          "snyk",
          "soheil",
          "strikethrough",
          "stringAlphadash",
          "stringAlphanum",
          "stringEnum",
          "supertest",
          "tabnine",
          "tabpanel",
          "teletalk",
          "Teletalk",
          "Trez",
          "typeof",
          "uncategorizedErrors",
          "upsert",
          "validators",
          "verifier",
          "vsintellicode",
          "WITHSCORES",
          "zadd",
          "zoot",
          "zrange"
        ],
        "skipIfMatch": ["http://[^s]*", "^[-\\w]+/[-\\w\\.]+$"],
        // "skipWordIfMatch": ["^foobar.*$"],
        "minLength": 4
      }
    ]
  }
}
